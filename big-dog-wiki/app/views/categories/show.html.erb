<div class="category-container see-through">
<!--   <span class="title thicker">Category: <%= @category.name %></span> -->

  <div class="title thicker category-articles">
    <%= @category.name %> Articles:
      <div class="cat-arts subtitle">

      <!-- We need to only grab the last one, push it into an array, then loop over and display the article -->

        <% @category.versions.where(published: true).each do |article| %>
            <%= link_to article.title, article_path(article) %>
        <% end %>

      </div>
  </div>
</div>


